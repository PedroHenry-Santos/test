{
  "presets": [
    "next/babel",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["styled-components", {
      "ssr": true,
      "displayName": true
    }],
    "inline-react-svg"
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "styled-components", {
            "ssr": false,
            "displayName": false
          }
        ]
      ]
    }
  }
}
